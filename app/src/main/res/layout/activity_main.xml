<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.bl4deofsoul.starthing.MainActivity">

    <Button
        android:id="@+id/Func1"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:text="Convert Numbering to Star"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="52dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Star Location"
        app:layout_constraintTop_toBottomOf="@+id/Func2"
        tools:layout_editor_absoluteX="8dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Zodiac Fortune telling"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        tools:layout_editor_absoluteX="8dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TIps"
        app:layout_constraintTop_toBottomOf="@+id/button4"
        tools:layout_editor_absoluteX="8dp" />

    <Button
        android:id="@+id/Func2"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Convert Star to Numbering"
        app:layout_constraintTop_toBottomOf="@+id/Func1"
        tools:layout_editor_absoluteX="8dp" />
</android.support.constraint.ConstraintLayout>
